    "type": "package",
    "require": {
        "php": ">=5.6.4",
        "league/flysystem": "^1.0",
        "yespbs/egnyte": "*"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^2.4@dev",
        "phpunit/phpunit": "~5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Priyabrata Sarkar",
